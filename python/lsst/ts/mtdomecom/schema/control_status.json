{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "commandId": {
      "type": "number",
      "description": "The command ID as generated by the CSC."
    },
    "response": {
      "type": "number"
    },
    "LLC": {
      "type": "object",
      "properties": {
        "control_mode": {
          "enum": [
            "remote",
            "local_pushbuttons",
            "local_keba",
            "local_eui"
          ]
        }
      },
      "required": [
        "control_mode"
      ],
      "additionalProperties": false
    }
  },
  "required": [
    "commandId",
    "response",
    "LLC"
  ],
  "additionalProperties": false
}